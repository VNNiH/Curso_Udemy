algoritmo
var
    largura, comprimento, valorMetro, area, preco : real
inicio
        escreval("largura")
        leia(largura)
        escreval("comprimento")
        leia(comprimento)
        escreval("valor do Metro Quadrado")
        leia(valorMetro)
        area <- largura * comprimento
        preco <- area * valorMetro
        escreval(area)
        escreval(preco)
        
fimalgoritmo
