algoritmo

var

    nome1, nome2 : caractere
    idade1, idade2 : inteiro
    media : real

inicio

    escreva("Nome: ")
    leia(nome1)
    escreva("Idade: ")
    leia(idade1)

    escreva("Nome: ")
    leia(nome2)
    escreva("Idade: ")
    leia(idade2)

    media <- (idade1 + idade2) / 2
    
    escreval(media)

fimalgoritmo
